[alpine]
alpine01 ansible_host=172.17.0.4  ansible_user=root  ansible_password=xxx ansible_ssh_extra_args='-o StrictHostKeyChecking=no'
[centos]
centos01 ansible_host=172.17.0.3  ansible_user=centos  ansible_password=xxx ansible_ssh_extra_args='-o StrictHostKeyChecking=no'
[ubuntu]
ubuntu01 ansible_host=172.17.0.2  ansible_user=ubuntu  ansible_password=xxx ansible_ssh_extra_args='-o StrictHostKeyChecking=no'
[centos-remote]
remote-1 ansible_host=0.0.0.0  ansible_ssh_user=centos ansible_ssh_private_key=/home/centos/.ssh/id_rsa
